<%- include('../partials/header') %>

<h1>Austin Attractions</h1>

<table id="list">

    <thead>
      <tr>
        <th></th>
        <th>Title</th>
        <th>Address</th>
        <th>Description</th>
        <th></th>
      </tr>
    </thead>

    <div>
      <tbody>
      <% attractions.forEach(function(attraction) { %>
        <tr>
          <td>
            <img src=<%= attraction.image %> />
          </td>
          <td>
            <%= attraction.title %>
          </td>
          <td>
            <%= attraction.address %>
          </td>
          <td>
            <%= attraction.description %>
          </td>
          <td>
            <a href="/attractions/<%= attraction._id %>">DETAILS</a>
          </td>
        </tr>
        <% }); %>
      </tbody>
    </div>

  </table>
